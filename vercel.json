{
    "buildCommand": "vite build",
    "outputDirectory": "dist",
    "framework": "vite",
    "functions": {
        "api/*.ts": {
            "runtime": "@vercel/node@3"
        }
    },
    "rewrites": [
        {
            "source": "/((?!api/).*)",
            "destination": "/index.html"
        }
    ]
}